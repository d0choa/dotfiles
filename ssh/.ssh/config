Host ebi
     User ochoa
     HostName ebi-login.ebi.ac.uk
     ForwardX11 yes
     ForwardX11Trusted yes
     ProxyCommand ssh -l ochoa -Y gate.ebi.ac.uk proxy %h

Host ebicli
     User ochoa
     HostName ebi-cli-001.ebi.ac.uk
     ForwardX11 yes
     ForwardX11Trusted yes
     ProxyCommand ssh -l ochoa -Y gate.ebi.ac.uk proxy %h

Host ebi_tunnel
     HostName gate.ebi.ac.uk
     User ochoa
     LocalForward 6622 bilbao.windows.ebi.ac.uk:22

#requires ebi_tunnel connection
Host bilbao
     Hostname localhost
     User ochoa
     Port 6622
